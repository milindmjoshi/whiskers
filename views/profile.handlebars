<div class="profile-name-container row">
  <div class="col-auto search-and-name">
    <h2 class="profile-name text-white">Welcome, {{name}}!</h2>
    <input class="whisky-search" type="text" placeholder="search whiskys">
  </div>
</div>

<div class="container-fluid profile-container">
  <div class="row">

    <div class="my-whiskys-container col-12 col-sm-6">
      <div class="my-whiskys-title-container">
        <h2 class="my-whiskys-title">My Whiskys</h2>
      </div>
      <div class="my-whiskys-content-container">
        {{> whisky-card }}
      </div>
    </div>
    <div class="whisky-feed-container col-12 col-sm-6">
      <div class="whisky-feed-title-container">
        <h2 class="whisky-feed-title">Whisky Feed</h2>
      </div>
       <div class="my-whisky-feed-content-container">
        {{> whisky-feed }}
      </div>
    </div>
  </div>
</div>

<script src="./js/profile.js"></script>